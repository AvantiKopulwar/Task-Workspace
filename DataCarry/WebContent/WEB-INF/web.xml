<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">
	<servlet>
		<servlet-name>dask</servlet-name>
		<servlet-class>com.av.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>dask</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ask</servlet-name>
		<servlet-class>com.av.InboxServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ask</servlet-name>
		<url-pattern>/log</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>sk</servlet-name>
		<servlet-class>com.av.DraftServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>sk</servlet-name>
		<url-pattern>/on</url-pattern>
	</servlet-mapping>
</web-app>